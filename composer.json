{
  "name": "bondacom/antenna",
  "description": "Laravel Wrapper to OneSignal REST API.",
  "require": {
    "php": "^7.1",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "~3.4",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Bondacom\\Antenna\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bondacom\\Antenna\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bondacom\\Antenna\\Providers\\AntennaServiceProvider"
      ],
      "aliases": {
        "Antenna": "Bondacom\\Antenna\\Facades\\Antenna"
      }
    }
  }
}
