{
  "name": "Bondacom/antenna",
  "description": "Laravel Wrapper to OneSignal REST API.",
  "type": "library",
  "homepage": "https://bondacom.com",
  "authors": [
    {
      "name": "Bondacom Dev Team",
      "email": "hello@bondacom.com",
      "homepage": "https://www.bondacom.com",
      "role": "Owner"
    }
  ],
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "orchestra/testbench": "~3.4",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Bondacom\\Antenna\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bondacom\\Antenna\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bondacom\\Antenna\\Providers\\AntennaServiceProvider"
      ],
      "aliases": {
        "Antenna": "Bondacom\\Antenna\\Facades\\Antenna"
      }
    }
  }
}
